<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#12ffffff" android:endColor="#12ffffff" android:angle="90.0" android:centerY="0.5" android:centerColor="#12ffffff" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#78ffffff" android:endColor="#78ffffff" android:angle="90.0" android:centerY="0.5" android:centerColor="#78ffffff" />
            </shape>
        </clip>
    </item>
</layer-list>